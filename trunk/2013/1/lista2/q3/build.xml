<?xml version="1.0" encoding="UTF-8"?>
<project name="q3">
	<property file="../correcaoL2/source.properties" />
	<property name="question.number" value="3"/>

	<target name="clean">
		<echo message="clean..." />
		<delete>
		    <fileset dir="." includes="*.c,s*.txt"/>
		</delete>
		<delete dir="Debug" />
		<delete dir="Release" />
		<echo message="done" />
	</target>
	<target name="copy">
		<echo message="copy..." />
		<copy file="${source.driver}${source.dir}\q${question.number}\q${question.number}.c" tofile="q${question.number}.c" overwrite="true" failonerror="false"/>
		<echo message="done" />
	</target>
	<target name="all" depends="clean, copy" />
</project>